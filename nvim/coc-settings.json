{
	"coc.preferences.extensionUpdateCheck": "daily",
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"svelte",
		"css",
		"html",
		"json"
	],
	"diagnostic.errorSign": "✖",
	"diagnostic.warningSign": "⚠",
	"diagnostic.infoSign": "•",
	"diagnostic.hintSign": "•",
	"prettier.requireConfig": true,
	"svelte.enable-ts-plugin": true
}
